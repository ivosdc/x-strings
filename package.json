{
  "name": "x-strings",
  "private": true,
  "version": "1.0.0",
  "description": "X-Strings",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js && cp -Rf ./src/static ./www",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@capacitor/android": "^3.4.3",
    "@capacitor/core": "^3.4.3",
    "cordova-plugin-dialogs": "^2.0.2",
    "dom7": "^3.0.0",
    "es6-promise-plugin": "^4.2.2",
    "framework7": "^6.3.16",
    "framework7-svelte": "^6.3.16",
    "material-icons": "^1.10.8",
    "phonegap-plugin-media-stream": "^0.0.2",
    "skeleton-elements": "^3.5.0",
    "svelte": "^3.46.4",
    "swiper": "^6.8.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.4.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.40",
    "cpy-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "framework7-icons": "^5.0.5",
    "guitar-chords-viewer": "^0.0.11",
    "guitar-tuner": "^0.1.5",
